<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.yc.bilibili.dao.DemoDao">
    <select id="getId" parameterType="java.lang.String" resultType="java.util.Map">
        select *
        from demo
        where id = #{id}
    </select>
</mapper>